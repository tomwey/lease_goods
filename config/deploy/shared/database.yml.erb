<%= fetch(:rails_env) %>:
  adapter: postgis
  encoding: unicode
  postgis_extension: postgis
  schema_search_path: public,postgis
  reconnect: false
  database: <%= "#{fetch(:application)}_#{fetch(:rails_env)}" %>
  pool: 5
  username: <%= "#{fetch(:application)}_#{fetch(:rails_env)}" %>
  password: 
  host: localhost
  port: 5432